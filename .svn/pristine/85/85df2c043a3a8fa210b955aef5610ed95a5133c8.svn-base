package com.mqtt.bean.userAction;

import com.mqtt.bean.AbsReportMsg;

public class ChannelInfo extends AbsReportMsg
{

    /**
     * 当前播放频点
     */
    private  Integer playFre; 
    
    /**
     * 频道名称
     */
    private String channelName;
    
    /**
     * 频道serviceId
     */
    private  Integer serviceId;
    
    /**
     * 是否是音频广播
     */
    private Integer isRadio;
    

    public Integer getPlayFre()
    {
        return playFre;
    }

    public void setPlayFre(Integer playFre)
    {
        this.playFre = playFre;
    }

    public String getChannelName()
    {
        return channelName;
    }

    public void setChannelName(String channelName)
    {
        this.channelName = channelName;
    }

    public Integer getServiceId()
    {
        return serviceId;
    }

    public void setServiceId(Integer serviceId)
    {
        this.serviceId = serviceId;
    }

    public Integer getIsRadio()
    {
        return isRadio;
    }

    public void setIsRadio(Integer isRadio)
    {
        this.isRadio = isRadio;
    }


}
